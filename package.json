  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-typescript": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "cross-env": "^5.2.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "typescript": "^3.4.3",
    "vue-template-compiler": "^2.5.21",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luz-alphacode/vue-ps-colorpicker.git"
  },
