{
  "name": "vue-ps-colorpicker",
  "version": "1.1.0",
  "description": "A photoshope style color picker in vue.",
  "author": "luz-alphacode <luz_alphacode@outlook.com>",
  "scripts": {
    "serve": "cross-env DEMO_TARGET=true vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name vue-ps-colorpicker ./src/lib.ts",
    "lint": "vue-cli-service lint",
    "build:demo": "cross-env DEMO_TARGET=true vue-cli-service build --dest gh-pages"
  },
  "main": "./dist/vue-ps-colorpicker.umd.js",
  "typings": "dist/types/lib.d.ts",
  "keywords": ["colorpicker", "color", "vue", "ps"],
  "dependencies": {
